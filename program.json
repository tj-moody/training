{
    "1": {
        "Type": "Progressive Overload Week 1",
        "Goal": "Start at moderate intensity",
        "Monday": {
            "Type": "High",
            "Goal": "Speed & Lower Body Power",
            "Work": {
                "Warm up": "15 min dynamic mobility + sprint buildups",
                "A skips": "3x10m",
                "Butt kicks": "3x10m",
                "Sprints": "3x10m acceleration starts (2 min rest)",
                "Flying sprints": "3x20m (3 min rest)",
                "Depth jumps": "3x3 from 18\" box",
                "Broad jumps": "3x4",
                "Single-leg bounds": "2x5 each leg",
                "Trap Bar Deadlift": "4x4 @ 80% 1RM",
                "Bulgarian Split Squat": "3x6 each leg",
                "Nordic Curls": "3x5",
                "Hanging Leg Raises": "3x10"
            }
        },
        "Tuesday": {
            "Type": "Low",
            "Goal": "Tempo & Recovery",
            "Work": {
                "Foam Rolling": "10 min (quads, glutes, calves)",
                "Banded Stretches": "10 min (hip flexors, hamstrings)",
                "Tempo Runs": "10x100m @ 70% effort, 30s walk rest",
                "Optional Skill": "Easy throwing or cone footwork, 15 min"
            }
        },
        "Wednesday": {
            "Type": "High",
            "Goal": "Agility & Reactive Strength",
            "Work": {
                "Warm up": "15 min mini hurdles, lateral skips, sprint buildups",
                "5-10-5 shuttle": "3 reps",
                "T-drill": "2 reps",
                "Reactive mirror drill": "3x10s bouts",
                "Depth to lateral bound": "3x3 each side",
                "Lateral hurdle hops": "3x4 per direction",
                "Back Squat": "4x3 @ 80–85% 1RM",
                "Step-ups with dumbbells": "3x6 each leg",
                "Romanian Deadlift": "3x8",
                "Ab Rollouts": "3x10"
            }
        },
        "Thursday": {
            "Type": "Low",
            "Goal": "Mobility & Aerobic Base",
            "Work": {
                "CARs (Controlled Articular Rotations)": "10 min",
                "Long-hold stretches": "15 min (hip flexors, adductors, hamstrings)",
                "Zone 2 Cardio": "30 min jog/bike/row",
                "Box breathing": "5 min (4-4-4-4 rhythm)"
            }
        },
        "Friday": {
            "Type": "High",
            "Goal": "Sprint Mechanics & Upper Body Strength",
            "Work": {
                "Sprint drills": "A-skips, B-skips, Fast feet - 2x15m each",
                "Sled push or hill sprints": "4x10m",
                "Depth jump to vertical": "3x3",
                "Pogo hops": "3x10",
                "Pull-ups": "4x6 (weighted if possible)",
                "Overhead press": "3x6",
                "Inverted rows": "3x12",
                "Farmer carries": "3x20m",
                "Cable anti-rotation press": "3x10 per side"
            }
        },
        "Saturday": {
            "Type": "Low",
            "Goal": "Skill & Active Recovery",
            "Work": {
                "Light throwing drills": "15 min",
                "Cone footwork or shadowing": "10-15 min",
                "Stretching": "20 min",
                "Optional": "Easy pickup or disc golf"
            }
        },
        "Sunday": {
            "Type": "Rest",
            "Goal": "Full Recovery",
            "Work": {
                "Gentle walk or bike": "20 min",
                "Breathing & mindfulness": "10 min",
                "Optional yoga or contrast therapy": "As desired"
            }
        }
    },

